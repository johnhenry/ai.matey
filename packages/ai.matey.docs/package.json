{
  "name": "ai.matey.docs",
  "version": "0.1.0",
  "description": "Documentation site for AI Matey - Universal AI Adapter System",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prebuild": "npm run build:api-docs",
    "build": "docusaurus build",
    "build:examples": "echo 'Skipping example validation for now'",
    "build:api-docs": "tsx scripts/generate-all-api-docs.ts && npm run typedoc:generate && npm run typedoc:fix-mdx",
    "typedoc:generate": "typedoc --options typedoc-all.json",
    "typedoc:fix-mdx": "tsx scripts/fix-mdx-compat.ts",
    "api:generate": "tsx scripts/generate-all-api-docs.ts && npm run typedoc:generate && npm run typedoc:fix-mdx",
    "api:watch": "tsx watch scripts/generate-all-api-docs.ts",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "examples:validate": "tsx scripts/validate-examples.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf build .docusaurus docs/api/generated docs/api/packages"
  },
  "dependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@mdx-js/react": "^3.0.1",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.5.2",
    "@docusaurus/tsconfig": "^3.5.2",
    "@docusaurus/types": "^3.5.2",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.12",
    "ai.matey.backend": "*",
    "ai.matey.cli": "*",
    "ai.matey.core": "*",
    "ai.matey.frontend": "*",
    "ai.matey.http": "*",
    "ai.matey.middleware": "*",
    "ai.matey.react.core": "*",
    "ai.matey.react.hooks": "*",
    "ai.matey.types": "*",
    "ai.matey.utils": "*",
    "ai.matey.wrapper": "*",
    "tsx": "^4.19.2",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.7.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome versions",
      "last 3 firefox versions",
      "last 5 safari versions"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
