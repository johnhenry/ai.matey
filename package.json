{
  "name": "ai.matey-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "AI Matey Monorepo - Universal AI Adapter System",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:esm": "turbo run build:esm",
    "build:cjs": "turbo run build:cjs",
    "build:types": "turbo run build:types",
    "clean": "turbo run clean && rm -rf node_modules/.cache",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "format:check": "prettier --check \"packages/*/src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish",
    "release:staggered": "./scripts/staggered-publish.sh",
    "release:staggered:dry-run": "./scripts/staggered-publish.sh --dry-run"
  },
  "keywords": [
    "ai",
    "llm",
    "adapter",
    "openai",
    "anthropic",
    "gemini",
    "universal",
    "provider-agnostic",
    "typescript"
  ],
  "author": "AI Matey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnhenry/ai.matey.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@types/express": "^5.0.3",
    "@types/koa": "^3.0.0",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fastify": "^5.6.1",
    "hono": "^4.9.12",
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@10.0.0"
}
